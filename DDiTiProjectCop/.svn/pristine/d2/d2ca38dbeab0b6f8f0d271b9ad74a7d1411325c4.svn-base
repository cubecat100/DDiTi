<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.ddit.mapper.ILoginMapper">
	
	<select id="login" parameterType="memberVO" resultType="memberVO">
		select 
			mem_no,
			dprt_no,
			mem_id,
			mem_pw,
			mem_name,
			mem_gen,
			mem_bir,
			mem_ph,
			mem_email,
			mem_postcode,
			mem_addr1,
			mem_addr2,
			otp_key,
			mem_profile,
			mem_nation_code,
			mem_temp_pw,
			mem_role
		from member
    	where mem_id = #{memId}
        	and mem_pw = #{memPw}
	</select>
	
</mapper>