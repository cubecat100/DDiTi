				function fCkChg(pThis) {   // pThis는 해당 checkbox
					$.ajax({
						type: "get",
						url: "/DYUniv/student/classSchShare.ajax",
						data: {
							memNo: pThis.value
						},
						dataType: "json",  // 서버에서 받은 값(문자열) rslt = JSON.parse(rslt)
						success: function (res) {
							console.log("넘어온 값", res);
							if(pThis.checked){
								addSchedule(res,"m"+res[0].memNo);
							}else {
								removeSchedule(res,"m"+res[0].memNo);
							}
						}
					})
				}


