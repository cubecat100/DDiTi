				$(function () {
					contentScroll();

					var sendBtn = $("#sendBtn");
					var fileUploadBtn = $("#fileUploadBtn");

					var memNo = "${memNo }";
					var cmNo = "${cmNo }";

					var memName = "김고양"; /* "${member.memName }"; */

					var sock = new SockJS("/community");

					sock.onopen = function () {
						console.log("연결완료");
					}

					sock.onmessage = function (res) {
						var data = JSON.parse(res.data);

						if (memNo == data.memNo) {
							msgOnMe(data);
						} else {
							msgOnOther(data);
						}
						contentScroll();
					}

					sock.onclose = function () {
						console.log("연결 종료");
					}

					//메시지 전송버튼
					sendBtn.on("click", function () {
						console.log("send");

						var msg = $("#msg").val();

						if (msg == null || msg == "") {
							return false;
						}

						console.log("msg : " + msg)

						$("#msg").val("");

						data = {
							"cmNo": cmNo,
							"memNo": memNo,
							"chatMsg": msg,
							"chatSender": memName,
						}

						sock.send(JSON.stringify(data));
					})

					//파일 전송
					fileUploadBtn.on("click", function () {
						console.log("file");
					})


				})

