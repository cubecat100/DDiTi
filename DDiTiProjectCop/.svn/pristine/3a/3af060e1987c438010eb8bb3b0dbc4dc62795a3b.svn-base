<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %>
<div class="container-full">
	<!-- Content Header (Page header) -->	  
	<div class="content-header">
		<div class="d-flex align-items-center">
			<div class="me-auto">
				<h3 class="page-title">교수 관리</h3>
				<div class="d-inline-block align-items-center">
					<nav>
						<ol class="breadcrumb">
							<li class="breadcrumb-item"><a href="#"><i class="mdi mdi-home-outline"></i></a></li>
							<li class="breadcrumb-item" aria-current="page">수업 및 교직 관리</li>
							<li class="breadcrumb-item active" aria-current="page">교수 관리</li>
						</ol>
					</nav>
				</div>
			</div>	
		</div>
	</div>  

	<!-- Main content -->
	<section class="invoice printableArea">
	  <div class="row">
		
		<div class="col-12">
		
		  <div class="page-header">
			<h2 class="d-inline">
				<strong class="fs-30">교수 조회</strong>
			</h2>
			<div class="col-12">
			  <div class="bb-1 clearFix">
				<div class="text-end pb-15">
					<button class="btn btn-primary" type="button"> <span><i class="fa fa-user-circle"></i> 일괄생성</span> </button>
					<button id="print2" class="btn btn-primary" type="button"> <span><i class="fa fa-user-circle"></i> 개별생성</span> </button>
					<br><br>
					

					 <div class="lookup lookup-circle lookup-right">
					  <select name="" class="">
					  		<option value="none">=학과 =</option>
					  		<option value="#학과">학과1</option>
					  		<option value="#학과">학과2</option>
					  		<option value="#학과">학과3</option>	  		
					  	</select>
						<input type="text" data-provide="media-search" placeholder="교수명">
					  </div>

				</div>	
			  </div>
			</div>

		  </div>
		</div>
		<!-- /.col -->
	  </div>
	  <div class="row invoice-info">
		<div class="col-md-6 invoice-col">
		  <h5>
			<span class="text-blue fs-18">##학과  교수 목록</span>
		  </h5>
		</div>
		<!-- /.col -->
		<div class="col-md-6 invoice-col text-end">
		  <strong>To</strong>
		</div>
	  </div>
	  <br/>
	  <div class="row">
		<div class="col-12 table-responsive">
		  <table class="table table-bordered">
			<tbody>
			<tr>
			  <th>선택</th>
			  <th>번호</th>
			  <th>이름</th>
			  <th>학번</th>
			  <th>합격 전형</th>
			  <th>학과</th>
			</tr>		
			<c:forEach items="${newStuList }" var="member">
				<c:choose>
					<c:when test="${empty member }">
						<tr><td colspan="6">신입생 리스트가 없습니다.</td></tr>
					</c:when>
					<c:otherwise>
						<tr>
						  <td><input type="checkbox" name="" checked="checked"/></td>
						  <td>${member.memNo }</td>
						  <td>${member.memName }</td>
						  <td>${member.memId }</td>
						  <td>##</td>
						  <td>${member.dprtNo }</td>
						</tr>
					</c:otherwise>
				</c:choose>
			</c:forEach>
			</tbody>
		  </table>
		</div>
		<!-- /.col -->
	  </div>
	  <div class="row">
		<div class="col-12 text-end">
			<div>
				<p>총인원  : <span class="text-danger"> ## </span>명</p>
			</div>

		</div>
		<!-- /.col -->
	  </div>
	</section>
	<!-- /.content -->
</div>


