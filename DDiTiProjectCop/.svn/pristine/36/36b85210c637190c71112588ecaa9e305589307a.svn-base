<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.ddit.mapper.IFilesMapper">
	<insert id="createFile">
		<selectKey keyProperty="fileNo" resultType="int" order="BEFORE">
			select file_seq.nextval from dual
		</selectKey>
		insert into files (
			FILE_NO,
			FILE_GROUP_NO,
			FILE_NAME,
			FILE_PATH,
			FILE_UUID,
			FILE_MIME,
			FILE_DOWN_CNT,
			FILE_UPLOAD_DATE,
			FILE_FANCYSIZE,
			FILE_SIZE
		) values (
			#{fileNo}, 
			#{fileGroupNo}, 
			#{fileName}, 
			#{filePath}, 
			#{fileUuid}, 
			#{fileMime}, 
			0, 
			sysdate,
			#{fileFancysize}, 
			#{fileSize}
		)
	</insert>
</mapper>